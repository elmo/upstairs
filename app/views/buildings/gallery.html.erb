<h1>Photos</h1>
<gallery>
<left>
<% @building.photos.each do |photo| %>
 <%= cl_image_tag(photo.path, { size: '400x600', crop: :fit }) %>
<% end %>

<p><%= link_to 'back', building_path( @building) -%> </p>
</left>

<right>

<% @building.photos.each do |photo| -%>
  <%= cl_image_tag(photo.path, { size: '100x100', crop: :scale}) %>
<% end %>
</right>

</gallery>
