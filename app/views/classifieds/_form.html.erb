<%= simple_form_for([@community,@classified]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :category_id, collection: category_list  %>
    <%= f.input :title %>
    <%= f.input :body %>
    <%= f.input :photos, as: :attachinary %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
