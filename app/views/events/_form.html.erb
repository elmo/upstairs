<%= simple_form_for([@community, @event]) do |f| %>

  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :body %>
    <%= f.input :starts, as: :datetime_picker %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<script type="text/javascript">
   $('.datetimepicker').datetimepicker();
 </script>
